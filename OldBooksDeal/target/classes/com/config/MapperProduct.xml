<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mpProduct">
	<insert id="productBoardInsert" parameterType="ProductDTO">
		insert into PRODUCT_BOARD_DB(PBOARD_NO,RESISTDATE,TITLE,PCONTENTS,PVIEW,id,ISSELL,PRICE,PRODUCTTYPE,PRODUCT_LOCATION,PRODUCTIMAGE)
		values (seq_pboard_no.nextval,sysdate,#{title},#{pcontents},0,#{id},#{isSell},#{price},#{productType},#{productLocation},#{imageName})
	</insert>
</mapper>